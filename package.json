{
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "sideEffects": [
        "*.css"
    ],
    "name": "zigbee2mqtt-frontend",
    "version": "0.2.107",
    "license": "GPL-3.0",
    "scripts": {
        "build": "npm run clean && webpack --config ./webpack.config.js --mode production",
        "start": "concurrently 'npm run start:ws' 'npm run start:dev'",
        "start:dev": "webpack serve --config ./webpack.config.js --mode development",
        "start:ws": "node ws.js",
        "clean": "rimraf ./dist",
        "postversion": "git push && git push --tags",
        "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
        "pretty": "prettier --write \"./**/*.{js,jsx,json}\""
    },
    "eslintIgnore": [
        "dist/*"
    ],
    "dependencies": {},
    "devDependencies": {
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^11.0.0",
        "@fortawesome/fontawesome-free": "^5.15.1",
        "@teamsupercell/typings-for-css-modules-loader": "^2.4.0",
        "@types/classnames": "^2.2.10",
        "@types/color-convert": "^2.0.0",
        "@types/d3-drag": "^2.0.0",
        "@types/d3-force": "^2.1.0",
        "@types/d3-selection": "^2.0.0",
        "@types/d3-zoom": "^2.0.0",
        "@types/lodash": "^4.14.165",
        "@types/notyf": "^3.0.0",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/react-router-dom": "^5.1.6",
        "@types/reconnectingwebsocket": "^1.0.5",
        "@types/webpack-env": "^1.16.0",
        "@types/ws": "^7.4.0",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
        "@typescript-eslint/parser": "^2.34.0",
        "bootstrap": "^5.0.0-alpha3",
        "classnames": "^2.2.6",
        "color-convert": "^2.0.1",
        "concurrently": "^5.3.0",
        "conventional-changelog-cli": "^2.1.1",
        "copy-webpack-plugin": "^6.4.0",
        "css-loader": "^5.0.1",
        "d3-drag": "^2.0.0",
        "d3-force": "^2.1.1",
        "d3-force-reuse": "^1.0.1",
        "d3-selection": "^2.0.0",
        "d3-zoom": "^2.0.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-config-react": "^1.1.7",
        "eslint-plugin-prettier": "^3.2.0",
        "eslint-plugin-react": "^7.21.5",
        "file-loader": "^6.2.0",
        "fork-ts-checker-webpack-plugin": "^6.0.5",
        "html-webpack-plugin": "^4.5.0",
        "husky": "^4.3.5",
        "line-reader": "^0.4.0",
        "lodash": "^4.17.20",
        "mini-css-extract-plugin": "^1.3.3",
        "notyf": "^3.9.0",
        "prettier": "^2.2.1",
        "raw-loader": "^4.0.2",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-router-dom": "^5.2.0",
        "reconnecting-websocket": "^4.4.0",
        "rimraf": "^3.0.2",
        "timeago.js": "^4.0.2",
